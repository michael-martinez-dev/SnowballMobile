{
  "expo": {
    "name": "SnowballMobile",
    "slug": "SnowballMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/snowflake.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "buildType": "apk",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/snowflake.png",
        "backgroundColor": "#ffffff"
      },
      "androidNavigationBar": {
        "visible": true
      },
      "package": "com.anonymous.SnowballMobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/snowflake.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/snowflake.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9fd171a1-fe97-4375-86d1-d7fb2e7e77c3"
      },
      "pocketbaseUrl": "http://10.0.0.35:8090"
    }
  }
}
